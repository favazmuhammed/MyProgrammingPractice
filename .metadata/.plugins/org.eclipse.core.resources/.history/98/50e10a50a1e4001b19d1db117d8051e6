#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1, num2;
	scanf("%d%d", &num1, &num2);
	if (num1 > num2){
		printf("number 1 is greater");
	}

	else {
		printf("Number 2 is greater");
	}
	return EXIT_SUCCESS;
}
