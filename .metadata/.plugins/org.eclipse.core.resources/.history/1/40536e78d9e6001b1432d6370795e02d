#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int limit,i, a[100];

	printf("Enter the array limit");
	scanf("%d", &limit);

	printf("Input elements in the array\n");

	for (i = 0; i< limit; i++){

		scanf("%d", &a[i]);
	}
	printf("Print elements in the array\n");

	for(i = 0; i<limit; i++){

		printf("%d\n", a[i]);
	}

	return EXIT_SUCCESS;
}
