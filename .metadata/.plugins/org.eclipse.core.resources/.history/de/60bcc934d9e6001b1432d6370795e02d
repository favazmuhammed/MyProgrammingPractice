#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i, a[5];
	printf("Input elements in the array\n");

	for (i = 0; i<  5; i++){

		scanf("%d", &a[i]);
	}printf("Print elements in the array\n");

	for(i = 0; i<5; i++){

		printf("%d\n", a[i]);
	}

	return EXIT_SUCCESS;
}
